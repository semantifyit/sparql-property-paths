{
  "name": "sparql-property-paths",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "test": "jest --testMatch \"**/test/*.ts\"",
    "test-suite": "jest test/sparql_suite/runSuite.test.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
